generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Formulario {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  nombre   String
  email    String
  topic    String
  message  String
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}
